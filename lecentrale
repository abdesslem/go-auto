<script>
  window.readCookie = function readCookie(name) {
    var nameEQ = "".concat(name, "=");
    var ca = document.cookie.split(';');

    for (var i = 0; i < ca.length; i++) {
      var c = ca[i];

      while (c.charAt(0) === ' ') {
        c = c.substring(1, c.length);
      }

      if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
    }

    return null;
  };

  window.createCookie = function createCookie(name, value, days) {
    var expires = '';

    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
      expires = "; expires=".concat(date.toGMTString());
    }

    document.cookie = "".concat(name, "=").concat(value).concat(expires, "; path=/; samesite");
  };

  window._sp_ = {
    config: {
      accountId: 1348,
      wrapperAPIOrigin: "https://wrapper-api.sp-prod.net/tcfv2",
      mmsDomain: "https://message1348.sp-prod.net",
      domainName: "LC",
      events: {
        onMessageChoiceSelect: function (choice_id, choice_type_id) {
          switch (choice_type_id) {
            case 11:
              kvalueTarget = 'pubOk';
              break;
            case 13:
              kvalueTarget = 'pubKo';
              break;
            default:
              kvalueTarget = window.readCookie('kvalueTarget');
              break;
          }

          window.createCookie('kvalueTarget', kvalueTarget);
        },
        onPrivacyManagerAction: function (pmData) {
          if (pmData.purposeConsent === 'none' && pmData.vendorConsent === 'none') {
            kvalueTarget = 'pubKo';
          } else {
            kvalueTarget = 'pubOk';
          }
          window.createCookie('kvalueTarget', kvalueTarget);
        },
        onPMCancel: function () {
          window.createCookie('kvalueTarget', 'pubKo');
        }
      }
    }
  }
</script>
